timezone: timezone.o
	cc -g -o timezone timezone.o -lecal-1.2 -lxml2 -lglib-2.0

timezone.o: timezone.c
	cc -g -I/usr/include/glib-2.0 -I/usr/include/glib-2.0/glib -I/usr/lib/glib-2.0/include -I/usr/include/libxml2 -I/usr/include/evolution-data-server-1.6 -c timezone.c

clean:
	rm -fr timezone timezone.o
