objectclass ( zimbraTimeZone
	NAME 'zimbraTimeZone'
	DESC 'Time Zone info'
	SUP top STRUCTURAL
	MUST (
    	cn $
		zimbraTimeZoneStandardDtStart $
		zimbraTimeZoneStandardOffset)
	MAY (
		zimbraTimeZoneStandardRRule $
		zimbraTimeZoneDaylightDtStart $
		zimbraTimeZoneDaylightOffset $
		zimbraTimeZoneDaylightRRule
	))
