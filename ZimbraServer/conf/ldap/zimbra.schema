#
#
################
objectclass ( zimbraMimeEntry
	NAME 'zimbraMimeEntry'
	DESC 'MIME type info'
	SUP top STRUCTURAL
	MUST (
		cn $
		zimbraMimeType $ 
		zimbraMimeIndexingEnabled)
	MAY (description $
		zimbraMimeFileExtension $
		zimbraMimeHandlerClass)
	))

