<zimlet name="com_zimbra_collector" version="1.1" description="Contact Collector">
	<include>collector.js</include>
	<includeCSS>collector.css</includeCSS>
	<resource>collector-icon.gif</resource>
	<handlerObject>Com_Zimbra_Collector</handlerObject>

	<zimletPanelItem label="Contact Collector" icon="Collector-panelIcon">
		<toolTipText>Drag a conversation or message to collect contacts</toolTipText>
		<dragSource type="ZmConv" />
		<dragSource type="ZmMailMsg" />
	</zimletPanelItem>
</zimlet>
